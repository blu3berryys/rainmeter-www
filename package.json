{
  "name": "rainmeter-www",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "5.4.2"
  },
  "scripts": {
    "start": "hexo server --log",
    "generate": "hexo generate",
    "build": "npm run clean && npm run build:hexo && npm run build:css",
    "build:hexo": "hexo generate",
    "build:css": "postcss ./public/css/style.css --replace --verbose",
    "clean": "hexo clean"
  },
  "dependencies": {
    "bootstrap": "4.6.2",
    "ejs": "3.1.10",
    "hexo": "5.4.2",
    "hexo-renderer-dartsass": "0.1.2",
    "hexo-renderer-ejs": "2.0.0",
    "hexo-server": "3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "10.2.5",
    "browserslist": "4.24.3",
    "cssnano": "5.0.2",
    "postcss": "8.4.41",
    "postcss-cli": "11.0.0"
  }
}
